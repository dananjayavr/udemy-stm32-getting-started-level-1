#include "rcc.h"

/*
 * @brief HSE configuration
 */
void rcc_HSE_config(void) {
  /*
   * configuration parameters --> STM32F103 Clock tree
   * HSE = 8 MHz
   * PLL = x9 (72 MHz)
   * USB prescaler = /1.5
   * AHB prescaler = /1
   * Cortex prescaler (i.e. SysTick) = /1
   *
   * APB1 orescaler = /2 (36 Mhz, 72 MHz)
   * APB2 prescaler = /1 (72 MHz)
   * ADC prescaler = /6 (12 MHz)
   */
}
